{
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "create data",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tools/data_converter/vad_nuscenes_converter.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "nuscenes",
        "--root-path",
        "./data/nuscenes/trainval/mini",
        "--out-dir",
        "./data/nuscenes/trainval/infos",
        "--extra-tag",
        "vad_nuscenes",
        "--version",
        "v1.0-mini",
        "--canbus",
        "./data/nuscenes/trainval",
      ]
    }
  ]
}